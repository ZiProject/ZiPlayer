{
	"name": "ZiPlayer",
	"description": "A modular Discord voice player with plugin system for @discordjs/voice",
	"version": "1.0.0",
	"repository": {
		"url": "https://github.com/ZiProject/ZiPlayer",
		"type": "git"
	},
	"packages": [
		{
			"name": "ziplayer",
			"description": "Core player library with plugin system",
			"version": "1.0.0",
			"npm": "https://www.npmjs.com/package/ziplayer",
			"features": [
				"Plugin-based architecture",
				"Queue management",
				"Volume control",
				"Playback control",
				"Auto play",
				"Loop control",
				"Progress bar",
				"Event-driven",
				"Multi-guild support"
			]
